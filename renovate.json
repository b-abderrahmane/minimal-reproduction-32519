{
    "$schema": "https://docs.renovatebot.com/renovate-schema.json",
    "extends": [
      "config:base",
      ":dependencyDashboard"
    ],
    "forkProcessing": "enabled",
    "kustomize": {
      "fileMatch": [
        "(^|/)kustomization\\.ya?ml$",
        "(^|/)([a-zA-Z]+-)+chart\\.ya?ml$"
        ]
    },
    "packageRules": [
      {
        "matchPackageNames": [
          "*"
        ]
      }
    ]
  }
